{
  "private": true,
  "name": "textcat-ng",
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@4.7.0",
  "license": "GPL-3.0-or-later",
  "author": {
    "name": "Simon Legner",
    "email": "Simon.Legner@gmail.com"
  },
  "homepage": "https://gitlab.com/albina-euregio/textcat-ng",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/albina-euregio/textcat-ng.git"
  },
  "bugs": {
    "url": "https://gitlab.com/albina-euregio/textcat-ng/-/issues"
  },
  "scripts": {
    "prepare": "husky",
    "build": "vue-tsc --build & vite build",
    "build-editor": "vue-tsc --build & VITE_TEXTCAT_EDITOR=1 vite build",
    "dev": "vite",
    "dev-editor": "VITE_TEXTCAT_EDITOR=1 vite",
    "lint": "eslint src/",
    "test": "vitest run"
  },
  "lint-staged": {
    "*.{css,md,scss}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "dependencies": {
    "@vueuse/core": "^11.2.0",
    "highlight-words-core": "^1.2.3",
    "idb-keyval": "^6.2.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/highlight-words-core": "^1.2.3",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-typescript": "^14.1.3",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.31.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.2.2",
    "vitest": "^3.0.9",
    "vue-tsc": "^2.1.10"
  }
}
